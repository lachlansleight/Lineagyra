                        <ui:Label text="30s" display-tooltip-when-elided="True" name="sliderMax" style="position: absolute; top: 14px; left: auto; right: 0; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />

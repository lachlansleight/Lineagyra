                <ui:Button text="Pattern" display-tooltip-when-elided="True" name="pattern" />
                <ui:Button text="Generator" display-tooltip-when-elided="True" name="generator" />
                <ui:Button text="Advanced" display-tooltip-when-elided="True" name="advanced" />
            </ui:VisualElement>
            <ui:VisualElement name="body" style="flex-grow: 1; padding-left: 16px; padding-right: 16px;">
                <ui:VisualElement name="cameraContainer" class="pageContainer" style="flex-grow: 1; justify-content: flex-start; visibility: visible; display: flex;">
                    <ui:Slider picking-mode="Ignore" label="Field of View" value="90" high-value="179" name="fieldOfView" low-value="1" class="control">
                        <ui:Label text="1" display-tooltip-when-elided="True" name="sliderMin" style="position: absolute; top: 14px; left: 155px; right: auto; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                        <ui:Label text="179" display-tooltip-when-elided="True" name="sliderMax" style="position: absolute; top: 14px; left: auto; right: 0; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                    </ui:Slider>
                    <ui:Toggle label="Bloom" name="bloomToggle" value="true" class="control" style="color: rgba(255, 255, 255, 255);" />
                    <ui:Slider picking-mode="Ignore" label="Bloom Strength" value="1" high-value="2" name="bloomStrength" class="control">
                        <ui:Label text="0" display-tooltip-when-elided="True" name="sliderMin" style="position: absolute; top: 14px; left: 155px; right: auto; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                        <ui:Label text="2" display-tooltip-when-elided="True" name="sliderMax" style="position: absolute; top: 14px; left: auto; right: 0; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                    </ui:Slider>
                    <ui:Slider picking-mode="Ignore" label="Bloom Threshold" value="0.8" high-value="1" name="bloomThreshold" class="control">
                        <ui:Label text="0" display-tooltip-when-elided="True" name="sliderMin" style="position: absolute; top: 14px; left: 155px; right: auto; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                        <ui:Label text="1" display-tooltip-when-elided="True" name="sliderMax" style="position: absolute; top: 14px; left: auto; right: 0; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                    </ui:Slider>
                    <ui:Slider picking-mode="Ignore" label="Pattern Size" value="1.25" high-value="2" low-value="0.5" name="patternSize" class="control">
                        <ui:Label text="200%" display-tooltip-when-elided="True" name="sliderMax" style="position: absolute; top: 14px; left: auto; right: 0; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                        <ui:Label text="50%" display-tooltip-when-elided="True" name="sliderMin" style="position: absolute; top: 14px; left: 155px; right: auto; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                    </ui:Slider>
                </ui:VisualElement>
                <ui:VisualElement name="patternContainer" class="pageContainer" style="flex-grow: 1; justify-content: flex-start; visibility: visible; display: none;">
                    <ui:Slider picking-mode="Ignore" label="Line Interval" value="4" high-value="20" name="lineInterval" low-value="1" class="control">
                        <ui:Label text="1" display-tooltip-when-elided="True" name="sliderMin" style="position: absolute; top: 14px; left: 155px; right: auto; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                        <ui:Label text="20" display-tooltip-when-elided="True" name="sliderMax" style="position: absolute; top: 14px; left: auto; right: 0; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                    </ui:Slider>
                    <ui:Slider picking-mode="Ignore" label="Time Step" value="0.02" high-value="0.05" name="timeStep" low-value="0.001" class="control">

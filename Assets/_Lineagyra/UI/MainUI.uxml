                        <ui:Label text="Oscillate" display-tooltip-when-elided="True" name="sliderMid" style="position: absolute; top: 14px; left: 242.2px; right: auto; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                        <ui:Label text="Linear" display-tooltip-when-elided="True" name="sliderMax" style="position: absolute; top: 14px; left: auto; right: 0; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                    </ui:Slider>
                    <ui:Slider picking-mode="Ignore" label="Auto Shuffle Period" value="1" high-value="2" name="autoShufflePeriod" class="control">

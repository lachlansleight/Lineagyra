                        <ui:Label text="0%" display-tooltip-when-elided="True" name="sliderMin" style="position: absolute; top: 14px; left: 155px; right: auto; font-size: 8px; -unity-text-align: upper-right; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0;" />
                    </ui:Slider>
                    <ui:Toggle label="Draw Fill" name="drawFill" value="true" class="control" style="color: rgb(255, 255, 255);" />
                    <ui:Slider picking-mode="Ignore" label="Fill Opacity" value="0.04" high-value="1" name="fillOpacity" class="control">

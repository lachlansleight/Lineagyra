<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <ui:VisualElement name="main" style="flex-grow: 1; align-items: center; background-color: rgba(0, 0, 0, 0);">
        <Style src="MainUI.uss" />
        <ui:VisualElement name="container" style="width: 400px; flex-grow: 1; background-color: rgba(0, 0, 0, 0.89); padding-top: 16px; color: rgb(41, 41, 41); font-size: 16px; display: flex;">
            <ui:Label text="Lineagyra" display-tooltip-when-elided="True" style="color: rgb(255, 255, 255); -unity-text-align: middle-center; margin-bottom: 16px; justify-content: center;" />
            <ui:VisualElement name="header" style="height: auto; width: auto; flex-direction: row; justify-content: center; align-items: center; background-color: rgba(0, 0, 0, 0.8); margin-bottom: 16px;">
